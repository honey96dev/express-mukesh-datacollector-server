.kt-grid.kt-grid--ver.kt-grid--root
    #kt_login.kt-grid.kt-grid--hor.kt-grid--root.kt-login.kt-login--v1
        .kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--desktop.kt-grid--ver-desktop.kt-grid--hor-tablet-and-mobile
            // begin::Aside
            .kt-grid__item.kt-grid__item--order-tablet-and-mobile-2.kt-grid.kt-grid--hor.kt-login__aside(style='background-image: url(images/signup-bg.jpg);')
                .kt-grid__item.mobile-only-center
                    a.kt-login__logo(href='#')
                        img.kt-login__logo-image(src='images/logo-def1.png')
            // begin::Aside
            // begin::Content
            .kt-grid__item.kt-grid__item--fluid.kt-grid__item--order-tablet-and-mobile-1.kt-login__wrapper
                // begin::Head
                .kt-login__head
                    span.kt-login__signup-label Already registered?
                    a.kt-login__signup-link(href='/users/login') Login
                // end::Head
                // begin::Body
                .kt-login__body
                    // begin::Signin
                    .kt-login__form
                        .kt-login__title
                            h3 Register a new account
                            //h6(style='margin-top: 4rem !important;') El sitio para administrar propiedades en alquiler,Â  sin problemas
                        // begin::Form
                        form.kt-form(action='' novalidate='novalidate')
                            .form-group
                                label.form-label Name
                                .kt-input-icon.kt-input-icon--left
                                    input#name.form-control(type='text' name='name' placeholder='Your Name' autocomplete='off')
                                    span.kt-input-icon__icon.kt-input-icon__icon--left
                                        // <span><i class="la la-envelope" aria-hidden="true"></i></span>
                                        span
                                            i.material-icons.material-icons-outlined.input-icon(aria-hidden='true') perm_identity
                            .form-group
                                label.form-label E-Mail
                                .kt-input-icon.kt-input-icon--left
                                    input#email.form-control(type='text' name='email' placeholder='Your E-Mail' autocomplete='off')
                                    span.kt-input-icon__icon.kt-input-icon__icon--left
                                        // <span><i class="la la-envelope" aria-hidden="true"></i></span>
                                        span
                                            i.material-icons.material-icons-outlined.input-icon(aria-hidden='true') email
                            .form-group
                                label.form-label Password
                                .kt-input-icon.kt-input-icon--left
                                    // <input class="form-control" type="password" placeholder="Your Password" name="password">
                                    .kt-input-icon.kt-input-icon--right
                                        input#password.form-control(type='password' name='password' placeholder='Your Password')
                                        span.kt-input-icon__icon.kt-input-icon__icon--right
                                            span.show-hide-password-button(toggle='#password')
                                                i.fa.fa-eye.input-icon(aria-hidden='true')
                                    span.kt-input-icon__icon.kt-input-icon__icon--left
                                        span
                                            i.material-icons.material-icons-round.input-icon(aria-hidden='true') vpn_key
                            // begin::Action
                            .kt-login__actions(style='margin-bottom: 0 !important;')
                                // <input id="kt_login_remember_me" type="checkbox" class="md-control">Recordarme</input>
                                label.kt-checkbox.kt-checkbox--info
                                    input#accept1.accept-term(type='checkbox' name='accept1')
                                    | Accept the
                                    a.blue(href='#')  Terms and Conditions
                                    |  and
                                    a.blue(href='#')  Privacy Policy
                                    | .
                                    span
                                // <button id="kt_login_signin_submit" class="btn btn-primary btn-elevate kt-login__btn-primary">Sign In</button>
                            .kt-align-center
                                button#kt_login_signup_submit.btn.btn-primary.btn-elevate.kt-login__btn-primary Signup
